# Environment Configuration
# Copy this file to .env and modify as needed

# Toggle between local and remote Strapi
USE_LOCAL_STRAPI=true

# Local Strapi URL (when USE_LOCAL_STRAPI=true)
STRAPI_BASE_URI=http://localhost:1337

# Remote/Live Strapi URL (when USE_LOCAL_STRAPI=false)  
STRAPI_REMOTE_URI=https://your-live-strapi-url.com

# Site URL
SITE_URL=https://yonk.online
